cd `dirname $0`
. ./box-settings

if [ -f video.mkv ] ; then echo ; else

sudo apt install youtube-dl
youtube-dl -o video https://www.youtube.com/watch?v=2fZ75lhaGT8

fi


./box-mount

cp video.mkv root/home/pi/

./box-umount


./box-up-wait

./ssh " sudo apt update "
./ssh " sudo apt install -y omxplayer "

./box-down





