cd `dirname $0`
. ./box-settings

if [ -f phantom.mkv ] ; then echo ; else

sudo apt isntall youtube-dl
youtube-dl -o phantom https://www.youtube.com/watch?v=2fZ75lhaGT8

fi


./box-mount

cp phantom.mkv root/home/pi/

./box-umount


./box-up-wait

./ssh " sudo apt update "
./ssh " sudo apt install -y omxplayer "

./box-down





